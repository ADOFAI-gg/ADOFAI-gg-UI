{
	"name": "@adofai-gg/ui",
	"version": "0.0.1",
	"packageManager": "pnpm@9.4.0",
	"scripts": {
		"dev": "pnpm generate && vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "pnpm generate && svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"storybook": "pnpm generate && storybook dev -p 6006",
		"build-storybook": "pnpm generate && storybook build",
		"generate": "node scripts/generateGlobImports.mjs"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^8.0.5",
		"@storybook/addon-interactions": "^8.0.5",
		"@storybook/addon-links": "^8.0.5",
		"@storybook/addon-storysource": "^8.0.9",
		"@storybook/addon-svelte-csf": "^4.1.2",
		"@storybook/blocks": "^8.0.5",
		"@storybook/manager-api": "^8.0.5",
		"@storybook/svelte": "^8.0.5",
		"@storybook/sveltekit": "^8.0.5",
		"@storybook/test": "^8.0.5",
		"@storybook/theming": "^8.0.5",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/js-cookie": "^3.0.6",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"chromatic": "^11.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-svelte": "^2.35.1",
		"glob": "^10.3.10",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"sass": "^1.72.0",
		"semantic-release": "^23.0.4",
		"storybook": "^8.0.5",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@fluent/bundle": "^0.18.0",
		"@fontsource/ibm-plex-sans-kr": "^5.0.13",
		"@fontsource/m-plus-rounded-1c": "^5.0.12",
		"@fontsource/quicksand": "^5.0.17",
		"@fontsource/roboto-mono": "^5.0.17",
		"esm-env": "^1.0.0",
		"js-cookie": "^3.0.5",
		"svelte-component-actions": "^1.1.0",
		"tippy.js": "^6.3.7"
	},
	"release": {
		"branches": [
			"+([0-9])?(.{+([0-9]),x}).x",
			"master",
			"next",
			{
				"name": "beta",
				"prerelease": true
			},
			{
				"name": "alpha",
				"prerelease": true
			}
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/npm",
			[
				"@semantic-release/github",
				{
					"successComment": false,
					"failTitle": false
				}
			]
		]
	}
}
